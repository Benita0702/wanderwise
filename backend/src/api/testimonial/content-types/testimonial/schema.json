    "Photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tour_packages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tour-package.tour-package",
      "inversedBy": "testimonials"
    },
    "user_profiles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::user-profile.user-profile",
      "inversedBy": "testimonials"
    }
  }
