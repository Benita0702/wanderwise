{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "UserProfile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "FullName": {
      "type": "string"
    },
    "Email": {
      "type": "email"
    },
    "Preferences": {
      "type": "json"
    },
    "wishlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tour-package.tour-package",
      "inversedBy": "user_profiles"
    },
    "Phone": {
      "type": "string"
    },
    "Address": {
      "type": "blocks"
    },
    "Birthday": {
      "type": "date"
    },
    "Gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Others"
      ]
    },
    "Profile": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
